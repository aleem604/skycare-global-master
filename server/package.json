{
  "name": "server",
  "version": "1.0.0",
  "description": "skycare global  web server ",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": "=9.11.2"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@cloudant/cloudant": "^3.0.2",
    "@loopback/authentication": "^1.0.6",
    "@loopback/boot": "^1.0.6",
    "@loopback/context": "^1.2.0",
    "@loopback/core": "^1.1.1",
    "@loopback/http-server": "^1.1.5",
    "@loopback/openapi-v3": "^1.1.3",
    "@loopback/repository": "^1.0.6",
    "@loopback/rest": "^1.4.0",
    "@loopback/rest-explorer": "^1.1.2",
    "@loopback/service-proxy": "^1.0.3",
    "@sendgrid/mail": "^6.3.1",
    "aws-sdk": "^2.460.0",
    "cfenv": "^1.1.0",
    "eslint-config-prettier": "^5.0.0",
    "express": "^4.16.4",
    "ibm-cos-sdk": "^1.4.1",
    "jwt-simple": "^0.5.5",
    "loopback-connector-cloudant": "^2.2.1",
    "loopback-connector-couchdb2": "^1.3.0",
    "loopback-connector-mongodb": "^4.2.0",
    "loopback-connector-mysql": "^5.3.1",
    "multer": "^1.4.1",
    "nexmo": "^2.4.0",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sha3": "^2.0.0",
    "socket.io": "^2.2.0",
    "speakeasy": "^2.0.0",
    "uuid62": "^1.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.0.1",
    "@loopback/testlab": "^1.0.1",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.11.2",
    "@types/passport": "^0.4.7",
    "@types/passport-http": "^0.3.6",
    "@types/passport-jwt": "^3.0.1",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.16",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "socket.io-client": "^2.2.0"
  }
}
